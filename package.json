{
  "name": "@dr-kobros/react-training-broilerplate",
  "version": "2.0.0",
  "description": "An example / training project broilerplate utilizing React et al.",
  "main": "src",
  "scripts": {
    "clean": "rimraf ./dist",
    "build:webpack": "NODE_ENV=production webpack --progress -p",
    "build:server": "NODE_ENV=production webpack --config webpack.config.server.babel.js --progress -p",
    "build": "npm run clean && npm run build:webpack",
    "lint": "eslint src --ext js --ext jsx",
    "babel-node": "babel-node",
    "start": "NODE_ENV=development ./node_modules/.bin/nodemon --exec npm run babel-node -w src/dev.js -- src/dev.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-register -r ignore-styles -r jsdom-global/register \"./src/**/*.tests.js\"",
    "flow": "flow",
    "serve": "NODE_ENV=production ./node_modules/.bin/nodemon --exec ./node_modules/.bin/babel-node src/server.js",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pekkis/react-training-broilerplate"
  },
  "keywords": [
    "React",
    "example",
    "boilerplate",
    "broilerplate",
    "training"
  ],
  "author": "Pekkis",
  "license": "BSD-3-Clause",
  "dependencies": {
    "axios": "^0.15.0",
    "body-parser": "^1.15.2",
    "classnames": "^2.2.5",
    "express": "^4.13.4",
    "immutable": "^3.8.1",
    "merge": "^1.2.0",
    "node-uuid": "^1.4.7",
    "normalize.css": "^5.0.0",
    "null-loader": "^0.1.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-fa": "^4.1.2",
    "react-gravatar": "^2.5.2",
    "react-hot-loader": "^3.0.0-beta.4",
    "react-html-document": "^3.1.0",
    "react-immutable-proptypes": "^2.1.0",
    "react-inspector": "^1.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "recompose": "^0.20.2",
    "redbox-react": "^1.2.4",
    "redux": "^3.5.2",
    "redux-promise-middleware": "^4.0.0",
    "redux-thunk": "^2.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.20.1",
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-react-app": "^0.2.1",
    "bluebird": "^3.4.6",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "empty": "^0.10.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-flowtype": "^2.18.1",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.2",
    "express-http-proxy": "^0.10.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "faker": "^3.1.0",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.33.0",
    "flow-typed": "^2.0.0",
    "html-webpack-plugin": "^2.16.1",
    "ignore-styles": "^5.0.1",
    "img-loader": "^1.2.2",
    "jsdom": "^9.5.0",
    "jsdom-global": "^2.1.0",
    "json-loader": "^0.5.4",
    "manifest-revision-webpack-plugin": "^0.3.0",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.13.0",
    "precss": "^1.4.0",
    "random-js": "^1.0.8",
    "react-addons-test-utils": "^15.3.1",
    "redux-logger": "^2.7.0",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "webpack": "^1.13.0",
    "webpack-assets-manifest": "^0.5.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
